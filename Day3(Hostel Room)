#include <iostream>
using namespace std;

int main() {
 int t;
 cin>>t;
 while(t--){
     int n,x;
     cin>>n>>x;
     int a[n];
     for(int i=0;i<n;i++){
         cin>>a[i];
     }
     int maxv=x;
     for(int i=0;i<n;i++){
         x+=a[i];
         if(x>maxv){
             maxv=x;
         }
     }
       cout<<maxv<<endl;
 }
 return 0;
}

