#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T;
    while (T--) {
        int A, B;
        cin >> A >> B;
        int third_number = 21 - (A + B);
        if (third_number >= 1 && third_number <= 10) {
            cout << third_number << endl;
        } else {
            cout << -1 << endl;
        }
    }
    return 0;
}
